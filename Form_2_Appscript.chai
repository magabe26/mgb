      var tag = menu.getTag("RESULTS", pos);
      if (tag.valid) { 
         if (tag.value.contains(year, false)) {                      
           menu = menu.removeAtPosition(tag.index, tag.length);
           pos = tag.index;
         }else {
           pos = tag.index + tag.length;
         }
      } else {
         break;
      }
   }

   return menu;
}

