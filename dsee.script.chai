		  }
		  showPreview(no,url,fun[no](){

		     showOperationSuccessDialog("Kiashiria cha upakiaji sasa ni: <font color='#71f090'>" + loadingIndicators[no] + "</font>");

onUrlVisited("#showGoogleAd");

		  });
