def addLinkToMenuXML(name,link,xml)
{
  var output = xml.remove("</NECTA_RESULTS>",false);
  var result = "<RESULTS><A HREF=\"" + link + "\">" + name + "</A></RESULTS>";
  output = output + result;
  return output + "</NECTA_RESULTS>";;  
}

def getUpdatedMenu(string url, string xml, string label)
{
    if(!hasInternetAccess()){
	   return xml;
	}
	var html = get(url);
	var output = "";
	var linkFound = false;
	output = addLinkToMenuXML("|--------- " +  label,"#",output);

	stR("http", false).seq(any(stR(".htm", false), "<>").plus()).seq(stR(".htm", false)).replaceInMapped(html, fun[output,linkFound](string link) {
		  var year = getYear(link,"");
		  if(year != 0){
			 output = addLinkToMenuXML(to_string(year),link,output);
			 linkFound = true;
		  }
		  return link;
	   }, 0, -1);

 var timeStamp = getTimestamp();
 var lastUpdated = "Menu updated on: " + getDate(timeStamp).remove("today,",false) + " " + getTime(timeStamp);
 output = addLinkToMenuXML("|--------- " + lastUpdated,"#",output);
	   
   
	if(linkFound){
	   output = "<?xml version='1.0' encoding='UTF-8'?><NECTA_RESULTS>" + output;
	   
	}   
	return (!linkFound) ? xml : output;
}


