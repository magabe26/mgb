def createSelectQuery(columns, table, whereColumn, like) {
   return "SELECT ".append(columns).append(" FROM ").append(table).append(" WHERE ").append(whereColumn).append(" LIKE '").append(like).append("'");
}

def createDeleteQuery(table, whereColumn, like) {
   return "DELETE FROM ".append(table).append(" WHERE ").append(whereColumn).append(" LIKE '").append(like).append("'");
}

def excuteBooleanDBQuery(query, fn) {
   excuteDBQuery(query, fun[fn](success, values) {
      fn(success);
   });
}

def deleteGenderPerformanceData(name)
{
  var query = createDeleteQuery("GenderWisePerformance", "name", name.replace("'","''",true));
  excuteBooleanDBQuery(query, fun[name](success) {
     if(success){
		 log("Gender performance data deleted , name = " + name );
	  } else {
		 log("Gender performance data deletion failed , name = " + name );
	 }
  });
}

def fetchNectaRankingAndPerformance(name,cb)
{
  var query = createSelectQuery("EXAMINATION_CENTRE_RANKING_XML,EXAMINATION_CENTRE_CLASS_PERFORMANCE_XML,EXAMINATION_CENTRE_SUBJECTS_PERFORMANCE_XML", "NectaRankingAndPerformance", "NAME", name.replace("'","''",true));
  excuteDBQuery(query, fun[cb](success, values) {
  if (success) {
	 forEachString(values, fun[cb](value) {
	    var parts = [];
		var fn = fun[parts](string s) {
		  parts.push_back(s);
	   };
	   forEachString(value.split(","), fn);
	   
	   if(parts.size() == 3){
	      var rankingPath = parts[0];
		  var classPath = parts[1];
		  var subjectsPath = parts[2];
		  
	     cb(rankingPath,classPath,subjectsPath);
	   }
   
	 });
  }
});
}

def deleteNectaRankingAndPerformance(name)
{
   fetchNectaRankingAndPerformance(name,fun[name](rankingPath,classPath,subjectsPath){
          deleteFile(rankingPath);
		  deleteFile(classPath);
		  deleteFile(subjectsPath);
		  
		  var query =  createDeleteQuery("NectaRankingAndPerformance", "name", name.replace("'","''",true));
		  excuteBooleanDBQuery(query, fun[name](success) {
		      if(success){
			     log("Necta ranking and performance data deleted , name = " + name );
			  } else {
			     log("Necta ranking and performance data deletion failed , name = " + name);
			  }
		  });
   });
 
}

def fetchSavedResultsData(cb)
{
  excuteDBQuery("SELECT NAME,XMLFILEPATH  FROM savedNectaIndividualPages", fun[cb](success, values) {
  if (success) {
	 forEachString(values, fun[cb](value) {
	    var parts = [];
		var fn = fun[parts](string s) {
		  parts.push_back(s);
	   };
	   forEachString(value.split(","), fn);
	   
	   if(parts.size() == 2){
	      var resultsName = parts[0];
		  var resultsPath = parts[1];
		  cb(resultsName,resultsPath);
	   }
	 });
  }
});
}


def deleteSavedResultData()
{
   fetchSavedResultsData(fun[](resultsName,resultsPath){
     //Avoid breaking UI
    if(getData("DATA_RECENTLY_SAVED_INDIVIDUAL_PAGE_NAME").equals(resultsName,false)){
	   return;
	}
	if(deleteFile(resultsPath)){
	  var query = createDeleteQuery("savedNectaIndividualPages", "NAME", resultsName.replace("'","''",true));
	  excuteBooleanDBQuery(query, fun[resultsName](success) {
	  if(success){
		 log("Results data deleted , name = " + resultsName );
		 deleteNectaRankingAndPerformance(resultsName);
		 deleteGenderPerformanceData(resultsName);
	  } else {
		 log("Results data deletion failed , name = " + resultsName );
	  }
	});
	}
	
   });
}

