def dateToFloat(string dateStr) {
   var parts = [];
   var fn = fun[parts](string s) {
      parts.push_back(s);
   };
   forEachString(dateStr.split("/"), fn);
   
    if (parts.size() == 3) {
        var day = parts[0].simplified();   // dd
        var month = parts[1].simplified(); // MM
        var year = parts[2].simplified();  // yyyy
        var combined = year + month + day;
        return combined.to_float();
    }
    return 0.0;
}

def doDataReset()
{

    var config = get("https://raw.githubusercontent.com/magabe26/mgb/master/data-reset.config").trim();
  if(config.isEmpty()){
     config = getData("DATA_RESET_DATE");
  }
  if(!config.isEmpty()){
		var count = 0;
		var i = config.indexOf("/",0,false);
		while(i != -1){
		 ++count;
		 i = config.indexOf("/",i+1,false);
		}

		if(count == 2){
		   DATA_RESET_DATE = config;
		   saveOrUpdateData("DATA_RESET_DATE",DATA_RESET_DATE);
		   log("Updated: DATA_RESET_DATE = " + DATA_RESET_DATE);
		}
  }

  var key = "DATA_RESET_DATE__".append(DATA_RESET_DATE.replace("/","_",false));
  var status = getData(key);
  if(!status.isEmpty()){
     return;
  }
  var today = dateToFloat(getDate(getTimestamp()).remove("Today,",false));
  var reset = dateToFloat(DATA_RESET_DATE);
  if(today >= reset){
     deleteSavedResultData();
     saveOrUpdateData(key,"1");
	   log("Data reset success!");
   }  
}

